{
	"Logging": {
		"LogLevel": {
			"Default": "Debug",
			"Microsoft": "Warning",
			"Microsoft.Hosting.Lifetime": "Information"
		}
	},

	"AtomicOptions": {
		/* collection name is required */
		"Collection": "YourCollectionName",

		/* string for specific rarity, or empty string for all rarities */
		"Rarity": "",

		/* prices (below) must be positive integers */
		"MinPrice": null,
		"MaxPrice": null,
		"MinMint": null,
		"MaxMint": null
	},

	"TelegramOptions": {
		/* put your BotId here (12345:foobaretc) */
		"BotId": "",
		/* put @channelname where to post to */
		"ChannelName": "@YourChannelName"
	},

	"DiscordOptions": {
		/* put your webhook here (empty string = no discord integration) */
		"Webhook": ""
	}

}